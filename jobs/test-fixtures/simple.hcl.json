{
	"name": "test",
	"groups": [{
		"name": "db",
		"count": 1,
		"tasks": [{
			"name": "db",
			"image": "redis:latest",
			"volumes": ["/var/run/docker.sock:/tmp/docker.sock"]
		}]
	}, {
		"name": "dummy",
		"count": 3,
		"tasks": [{
			"name": "dummy",
			"image": "alpine:latest"
		}]
	}, {
		"name": "registrator",
		"count": 1,
		"global": true,
		"tasks": [{
			"name": "registrator",
			"image": "gliderlabs/registrator:latest",
			"volumes": ["/var/run/docker.sock:/tmp/docker.sock"],
			"args": ["etcd://${COREOS_PRIVATE_IPV4}:4001/pulcy"]
		}]
	}, {
		"name": "web",
		"count": 2,
		"tasks": [{
			"name": "nginx",
			"image": "alpine:3.2",
			"args": [
				"ls",
				"-al"
			],
			"environment": {
				"key": "123",
				"name": "ewout",
				"envkey": "test-env",
				"cattest": "This is the content of file.txt"
			},
			"volumes-from": ["storage"],
			"http-check-path": "/",
			"frontends": [{
				"path-prefix": "/"
			}, {
				"domain": "foo.com"
			}, {
				"domain": "foo2.com",
				"path-prefix": "/foo2",
				"ssl-cert": "pulcy.pem"
			}]
		}, {
			"name": "storage",
			"image": "mystorage:latest"
		}]
	}]
}
