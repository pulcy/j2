[Unit]
Description=Main unit for test/web/nginx [slice 2]
Requires=docker.service
Requires=test-web-storage-mn@2.service
Requires=test-web-nginx-mn@2.service
After=docker.service
After=test-web-storage-mn@2.service

[Service]
Restart=always
RestartSec=1
StartLimitInterval=60s
StartLimitBurst=3
TimeoutStartSec=0
EnvironmentFile=/etc/environment
Environment="A00=--volumes-from test-web-storage-2"
Environment="A01=-e \"cattest=This is the content of file.txt\""
Environment="A02=-e \"envkey=test-env\""
Environment="A03=-e \"key=123\""
Environment="A04=-e \"name=ewout\""
Environment="A05=-e \"opttest1=value1\""
Environment="A06=-e \"opttest2=value2\""
Environment="A07=-e \"opttestenv=test-env\""
Environment="A08=-e \"quotetest=hello\""
Environment="A09=-e \"replacetest=1.2.3\""
Environment="A10=-e SERVICE_NAME=test-web-nginx"
Environment="A11=--log-driver=test"
ExecStartPre=/usr/bin/docker pull alpine:3.2
ExecStartPre=-/usr/bin/docker stop -t 10 test-web-nginx-2
ExecStartPre=-/usr/bin/docker rm -f test-web-nginx-2
ExecStart=/usr/bin/docker run --rm --name test-web-nginx-2 -P $A00 $A01 $A02 $A03 $A04 $A05 $A06 $A07 $A08 $A09 $A10 --add-host test.couchdb.couchdb.private:${COREOS_PRIVATE_IPV4} $A11 alpine:3.2 ls -al --db http://test.couchdb.couchdb.private:81
ExecStartPost=/bin/sh -c 'echo  | base64 -d | /usr/bin/etcdctl set /pulcy/frontend/test-web-nginx-2'
ExecStop=-/usr/bin/etcdctl rm /pulcy/frontend/test-web-nginx-2
ExecStop=-/usr/bin/docker stop -t 10 test-web-nginx-2
ExecStopPost=-/usr/bin/docker rm -f test-web-nginx-2

[X-Fleet]
MachineOf=test-web-backup-mn@2.service
MachineMetadata="web=true" "core=true"

[X-testproject]
GeneratedBy="testproject test-version, build test-build"
FrontEndRegistration="/pulcy/frontend/test-web-nginx-2={\"selectors\":[{\"path-prefix\":\"/\"},{\"weight\":10,\"domain\":\"foo.com\"},{\"weight\":12,\"domain\":\"foo2.com\",\"path-prefix\":\"/foo2\",\"ssl-cert\":\"pulcy.pem\",\"users\":[{\"user\":\"tester\",\"pwhash\":\"teROvYcTQSMgQ\"}]}],\"service\":\"test-web-nginx\",\"http-check-path\":\"/\"}"
